 int main()
        {
          float pi = 3.14;
          float area, raio;
          printf_s("Informe  o raio para o calculo:");
          scanf_s("%f", &raio);
          area = pi * (raio * raio);
          printf_s("Resultado do da raio:%.2f",area);
          system("pause");
          return 0;
}
