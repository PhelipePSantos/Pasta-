#include <stdio.h>
#include <stdlib.h>

        int main()
        {
          int x, n, i;
          printf_s("Informe um valor para x:\n");
          scanf_s("%i", &x);
          printf_s("Informe um valor para n:\n");
          scanf_s("%i", &n);
          for (i = 0; i < n - 1; i++)
          {
            x = x * x;
          }

          printf_s("%i", x);
          printf_s("\n");
          system("pause");
          return 0;
}
