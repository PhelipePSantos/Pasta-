        int main()
        {
          int num, i;
          printf_s("Informe um numero inteiro:\n");
          scanf_s("%i", &num);
          for (int i = 0; i < num; i++)
          {
            printf_s("%i", i * 2 + 1);
          }
          printf_s("\n");
          system("pause");
          return 0;
}
